{
    "problemMatcher": [
        {
            "owner": "luacheck-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^([^:]*):(\\d+):(\\d+-\\d+): \\(([E]\\d\\d\\d)\\) (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        },
        {
            "owner": "luacheck-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^([^:]*):(\\d+):(\\d+-\\d+): \\(([W]\\d\\d\\d)\\) (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
